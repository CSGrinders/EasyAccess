{
  "name": "easyaccess",
  "version": "1.0.0",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "NODE_OPTIONS='--no-deprecation' electron .",
    "build-main": "tsc --project tsconfig.main.json",
    "watch-main": "tsc --watch --project tsconfig.main.json",
    "dev": "webpack serve --config webpack.renderer.js",
    "build": "NODE_ENV=production webpack --config webpack.renderer.js",
    "start:server": "node dist/main/MCPServer.js",
    "build:full": "npm run build-main && npm run build",
    "pack": "npm run build:full && electron-builder --dir",
    "dist": "npm run build:full && electron-builder --publish=never",
    "dist:mac": "npm run build:full && electron-builder --mac --publish=never"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/electron-store": "^3.2.2",
    "@types/mime": "^3.0.4",
    "@types/mime-types": "^3.0.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "electron": "^35.1.5",
    "electron-builder": "^26.0.12",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.5",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.3",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.5",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@azure/msal-node": "^3.5.3",
    "@azure/msal-node-extensions": "^1.5.13",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@getstation/electron-google-oauth2": "^14.0.0",
    "@google/genai": "^1.4.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.8",
    "@radix-ui/react-slider": "^1.3.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-toggle": "^1.1.8",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/uuid": "^10.0.0",
    "archiver": "^7.0.1",
    "axios": "^1.9.0",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "diff": "^8.0.2",
    "dotenv": "^16.5.0",
    "dropbox": "^10.34.0",
    "electron-store": "^8.2.0",
    "framer-motion": "^12.15.0",
    "google-auth-library": "^9.15.1",
    "googleapis": "^148.0.0",
    "lucide-react": "^0.510.0",
    "mime": "^4.0.7",
    "mime-types": "^3.0.1",
    "minimatch": "^10.0.1",
    "next-themes": "^0.4.6",
    "pkce-challenge": "^5.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-pdf": "^9.2.1",
    "react-rnd": "^10.5.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.9",
    "uuid": "^11.1.0"
  },
  "build": {
    "appId": "com.easyaccess.app",
    "productName": "EasyAccess",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "public/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraFiles": [
      {
        "from": ".env",
        "to": ".env"
      }
    ],
    "asarUnpack": [
      "node_modules/keytar/**/*"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "identity": null
    },
    "dmg": {
      "title": "EasyAccess",
      "icon": false,
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
